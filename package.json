{
    "name": "backend",
    "version": "0.0.0",
    "private": true,
    "nodemonConfig": {
        "ignore": [
            "test.json",
            "*.log"
        ],
        "delay": "500"
    },
    "scripts": {
        "proxy": "nodemon --exec babel-node -r node_modules/dotenv/config ./proxy/cors.js dotenv_config_path=./config/.env",
        "api": "nodemon --exec babel-node -r node_modules/dotenv/config ./bin/www dotenv_config_path=./config/.env",
        "start": "npm run proxy && npm run api"
    },
    "dependencies": {
        "bluebird": "^3.7.2",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.4",
        "cors-anywhere": "^0.4.1",
        "debug": "~2.6.9",
        "dotenv": "^8.2.0",
        "dotenv-extended": "^2.7.0",
        "electron": "^11.5.0",
        "express": "^4.17.1",
        "express-redis-cache": "^1.1.3",
        "express-winston": "^4.0.1",
        "http-errors": "~1.6.3",
        "http-proxy-middleware": "^0.20.0",
        "morgan": "^1.9.1",
        "node": "^13.1.0",
        "node-fetch": "^2.6.1",
        "pug": "^2.0.4",
        "redis": "^2.8.0",
        "redis-rejson": "^1.0.0",
        "serve-favicon": "^2.5.0",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.7.5",
        "@babel/node": "^7.7.4",
        "@babel/preset-env": "^7.7.6",
        "@typescript-eslint/eslint-plugin": "^2.9.0",
        "@typescript-eslint/parser": "^2.9.0",
        "babel-loader": "^8.0.6",
        "eslint": "^6.7.2",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.7.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-prettier": "^3.1.1",
        "nodemon": "^2.0.2",
        "prettier": "^1.19.1",
        "standard": "^14.3.1"
    }
}
